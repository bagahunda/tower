html
  box-sizing border-box

*,
*:before,
*:after
  box-sizing inherit

body
  background-color red

:disabled, [disabled], [aria-disabled="true"]
  cursor not-allowed

.clearfix:after
    content ""
    display block
    clear both

.visuallyhidden:not(:focus):not(:active)
  position: absolute
  width: 1px
  height: 1px
  margin: -1px
  border: 0
  padding: 0
  clip-path: inset(100%)
  clip: rect(0 0 0 0)
  overflow: hidden

.svg-sprite
  display none
